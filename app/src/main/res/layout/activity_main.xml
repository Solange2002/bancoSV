+<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/animationView"
        android:layout_width="285dp"
        android:layout_height="292dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/bp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="81dp" />

    <LinearLayout
        android:id="@+id/Llnear1"
        android:layout_width="357dp"
        android:layout_height="60dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/animationView"
        tools:layout_editor_absoluteY="389dp">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#FFFFFF"
            app:srcCompat="@drawable/ic_usuario" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#FFFFFF"
            app:srcCompat="@drawable/ic_huella" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/ic_pin" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear2"
        android:layout_width="358dp"
        android:layout_height="37dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Llnear1"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="449dp">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="23dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Usuario y contraseña"
            android:textAlignment="center"
            android:textColor="#434860"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Huella/ Face ID"
            android:textAlignment="center"
            android:textColor="#434860"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="PIN"
            android:textAlignment="center"
            android:textColor="#434860"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear3"
        android:layout_width="358dp"
        android:layout_height="40dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/linear4"
        app:layout_constraintTop_toBottomOf="@id/linear2"
        tools:layout_editor_absoluteY="508dp">

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="#FFFFFF"
            android:tint="#000CF8"
            app:srcCompat="@drawable/ic_ubi" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="#FFFFFF"
            android:tint="#000CF8"
            app:srcCompat="@drawable/ic_clave" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="#FFFFFF"
            android:tint="#000CF8"
            app:srcCompat="@drawable/ic_cel" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear4"
        android:layout_width="355dp"
        android:layout_height="26dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/textView10"
        app:layout_constraintTop_toBottomOf="@id/linear3"
        tools:layout_editor_absoluteY="548dp">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="71dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Ubicanos"
            android:textAlignment="center"
            android:textColor="#000CF8" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Clave digital"
            android:textAlignment="center"
            android:textColor="#000CF8" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="72dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Llamanos"
            android:textAlignment="center"
            android:textColor="#000CF8" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="225dp"
        android:layout_height="31dp"
        android:text="¿Ingresar con otro usuario?"
        android:textAlignment="center"
        android:textColor="#000CF8"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/linear4"
        tools:layout_editor_absoluteY="605dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="340dp"
        android:layout_height="227dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bplogo"
        tools:layout_editor_absoluteY="-36dp" />
</androidx.constraintlayout.widget.ConstraintLayout>